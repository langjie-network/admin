<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0">
	<title>提示</title>
	<link href="../styles/weui.min.css" rel="stylesheet" />
</head>
<body>
	<div class="icon-box" style="text-align: center;margin-top: 30px;">
        <i class="weui-icon-info weui-icon_msg"></i>
        <div class="icon-box__ctn">
            <h3 class="icon-box__title">提示</h3>
            <p class="icon-box__desc"><%= sn %>已存在</p>
        </div>
    </div>
</body>
<%- include("../includes/scripts.ejs") %>
<script>
    var sn = '<%= sn %>';
    var type = '<%= type %>';
    setTimeout(function(){
        var r = window.confirm('该卡已存在，是否更新硬件信息？');
        if (type == 'vir') {
            if (r) {
                window.location.href = route('service/product/postInfoAgain');
            } else {
                window.location.href = route('service/product/vir8/'+sn);
            }
        } else if (type == 'dyna') {
            if (r) {
                window.location.href = route('service/product/postDynaAgain');
            } else {
                window.location.href = route('service/product/dyna/'+sn);
            }
        }
    }, 1000);
</script>
</html>